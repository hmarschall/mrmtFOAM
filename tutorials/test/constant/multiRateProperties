FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      multiRateProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

adaptiveTimeStepping    true;
maxCo  0.01;

immobileRegions
(
    immobile
    {
        type    FirstOrderRegions;
        numberOfTermsInExpansion    1;
    	alphaCoeffs uniform 1;
	    betaCoeffs  uniform 1;
    }
);
