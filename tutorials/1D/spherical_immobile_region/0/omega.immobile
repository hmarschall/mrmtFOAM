FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega.immobile;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 1e-2;

boundaryField
{

    "inlet|outlet"
    {
        type    calculated;
        value   $internalField;
    }

    frontAndBack
    {
        type            empty;
    }

}

